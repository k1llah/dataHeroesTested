    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "docs:dev": "vitepress dev VueSneakerShop",
    "docs:build": "vitepress build VueSneakerShop",
    "docs:preview": "vitepress preview VueSneakerShop"
  },
