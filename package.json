{
  "name": "vuesneakershop",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
