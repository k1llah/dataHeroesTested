    "axios": "^1.6.7",
    "crypto-js": "^4.2.0",
    "express": "^4.19.2",
    "gsap": "^3.12.5",
    "hooper": "^0.3.4",
    "js-md5": "^0.8.3",
    "lodash.debounce": "^4.0.8",
    "markdown-it": "^14.1.0",
    "marked": "^12.0.2",
